# Adds custom headers for specific resources
# This file is used by Netlify to handle headers

/assets/versions.json
  Access-Control-Allow-Origin: *

/assets/*.css
  Access-Control-Allow-Origin: https://cdpn.io

/post-icons/*.svg
  Access-Control-Allow-Origin: https://cdpn.io

/post-icons/*.png
  Access-Control-Allow-Origin: https://cdpn.io

/post-icons/*.jpg
  Access-Control-Allow-Origin: https://cdpn.io

/post-icons/*.jpeg
  Access-Control-Allow-Origin: https://cdpn.io

/post-icons/*.gif
  Access-Control-Allow-Origin: https://cdpn.io
