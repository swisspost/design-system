  "dependencies": {
    "@angular-devkit/core": "=15.0.4",
    "@angular-devkit/schematics": "=15.0.4",
    "@angular/core": "=15.0.4",
    "cheerio": "1.0.0-rc.12",
    "prettier": "^3.1.0",
    "typescript": "~4.9.3"
  },
  "devDependencies": {
    "@types/node": "^14.15.0"
  },
  "schematics": "./src/migrations.json",
  "ng-update": {
    "migrations": "./src/migrations.json"
  }
}
