    "@angular/platform-browser-dynamic": "19.2.18",
    "@angular/router": "19.2.18",
    "@primeng/themes": "19.0.9",
    "@swisspost/design-system-styles": "workspace:10.0.0-next.60",
    "primeng": "19.0.9",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
