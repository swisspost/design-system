{
  "name": "@swisspost/design-system-styles-primeng-workspace",
  "description": "Showcase for a Post like custom prime-ng theme",
  "version": "1.0.32",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm-run-all --parallel watch:lib serve:app",
    "build": "ng build styles-primeng",
    "watch:lib": "ng build --watch --configuration development styles-primeng",
    "serve:app": "ng serve --port 9325",
    "lint": "stylelint projects/styles-primeng/src/**/*.scss",
    "lint:fix": "stylelint projects/styles-primeng/src/**/*.scss --fix"
  },
  "dependencies": {
    "@angular/animations": "21.1.3",
    "@angular/common": "21.1.3",
    "@angular/compiler": "21.1.3",
    "@angular/core": "21.1.3",
    "@angular/forms": "21.1.3",
    "@angular/platform-browser": "21.1.3",
    "@angular/platform-browser-dynamic": "21.1.3",
    "@angular/router": "21.1.3",
    "@primeng/themes": "19.0.9",
    "@swisspost/design-system-styles": "workspace:9.6.0",
    "i": "0.3.7",
    "npm": "11.9.0",
    "primeng": "20.4.0",
    "rxjs": "7.8.2",
    "tslib": "2.6.3",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.2",
    "@angular/cli": "21.1.2",
    "@angular/compiler-cli": "21.1.3",
    "@types/jasmine": "5.1.4",
    "jasmine-core": "5.2.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "ng-packagr": "21.1.0",
    "npm-run-all2": "6.2.2",
    "stylelint": "16.8.1",
    "stylelint-config-sass-guidelines": "11.1.0",
    "stylelint-prettier": "5.0.2",
    "stylelint-scss": "6.4.1",
    "typescript": "5.9.3"
  }
}
