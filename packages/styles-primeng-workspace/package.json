    "@angular/platform-browser-dynamic": "19.2.0",
    "@angular/router": "19.2.0",
    "@primeng/themes": "19.0.9",
    "@swisspost/design-system-styles": "workspace:9.0.0-next.34",
    "primeng": "19.0.9",
    "rxjs": "7.8.1",
    "tslib": "2.8.1",
    "zone.js": "0.15.0"
  },
