{
  "name": "@swisspost/design-system-styles-primeng-workspace",
  "description": "Showcase for a Post like custom prime-ng theme",
  "version": "1.0.32",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm-run-all --parallel watch:lib serve:app",
    "build": "ng build styles-primeng",
    "watch:lib": "ng build --watch --configuration development styles-primeng",
    "serve:app": "ng serve --port 9325",
    "lint": "stylelint projects/styles-primeng/src/**/*.scss",
    "lint:fix": "stylelint projects/styles-primeng/src/**/*.scss --fix"
  },
  "dependencies": {
    "@angular/animations": "20.3.16",
    "@angular/common": "20.3.16",
    "@angular/compiler": "20.3.16",
    "@angular/core": "20.3.16",
    "@angular/forms": "20.3.16",
    "@angular/platform-browser": "20.3.16",
    "@angular/platform-browser-dynamic": "20.3.16",
    "@angular/router": "20.3.16",
    "@primeng/themes": "21.0.4",
    "@swisspost/design-system-styles": "workspace:9.6.0",
    "primeng": "21.0.4",
    "rxjs": "7.8.1",
    "tslib": "2.6.3",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.14",
    "@angular/cli": "20.3.14",
    "@angular/compiler-cli": "20.3.16",
    "@types/jasmine": "5.1.4",
    "jasmine-core": "5.2.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "ng-packagr": "20.3.2",
    "npm-run-all2": "6.2.2",
    "stylelint": "16.8.1",
    "stylelint-config-sass-guidelines": "11.1.0",
    "stylelint-prettier": "5.0.2",
    "stylelint-scss": "6.4.1",
    "typescript": "5.9.3"
  }
}
